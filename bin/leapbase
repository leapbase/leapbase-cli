#!/usr/bin/env node

const args = require('args');

args
  .command('create-project', 'Create project', ['create']);
args
 .command('add-extension', 'Add extension to project', ['add']);
args
.command('remove-extension', 'Remove extension from project', ['remove']);
args
.command('list-extension', 'List extensions for leapbase', ['list']);

const flags = args.parse(process.argv)
